{
    "name": "frontendj.ru",
    "version": "0.0.1",
    "homepage": "/",
    "private": true,
    "scripts": {
        "watch:sass": "npm run sass -- --watch",
        "watch:postcss": "postcss src/styles/index.css --use autoprefixer -d _site/styles --watch",
        "watch:eleventy": "eleventy --serve",
        "watch:js": "gulp watch",
        "build": "rm -rf _site && npm run sass --no-source-map && eleventy && npm run postcss && gulp build",
        "watch": "eleventy --watch",
        "serve": "eleventy --serve",
        "start": "npm-run-all --parallel watch:*",
        "debug": "DEBUG=* eleventy",
        "sass": "sass --style=compressed src/styles/index.scss src/styles/index.css",
        "postcss": "postcss src/styles/index.css --use autoprefixer --no-map -d _site/styles"
    },
    "license": "MIT",
    "devDependencies": {
        "@11ty/eleventy": "^0.12.1",
        "@11ty/eleventy-navigation": "^0.1.6",
        "@11ty/eleventy-plugin-rss": "^1.1.1",
        "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
        "luxon": "^1.26.0",
        "markdown-it": "^12.0.4",
        "markdown-it-anchor": "^7.1.0",
        "npm-run-all": "^4.1.5"
    },
    "dependencies": {
        "@babel/core": "^7.13.15",
        "@babel/preset-env": "^7.13.15",
        "autoprefixer": "^10.2.5",
        "del": "^6.0.0",
        "eleventy-critical-css": "^0.2.3",
        "eleventy-plugin-reading-time": "0.0.1",
        "fs": "^0.0.1-security",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-rev": "^9.0.0",
        "gulp-rev-rewrite": "^4.0.0",
        "gulp-terser": "^2.0.1",
        "html-minifier": "^4.0.0",
        "paths": "^0.1.1",
        "postcss": "^8.2.9",
        "postcss-cli": "^8.3.1",
        "sass": "^1.32.8",
        "terser": "^5.6.1",
        "vinyl-paths": "^3.0.1"
    }
}
